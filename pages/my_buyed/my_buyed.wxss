/* pages/my_buyed/my_buyed.wxss */

.container {
    background-color: #eee;
}

.cell {
    align-self: stretch;
    display: flex;
    flex-direction: column;
    align-items: flex-start;
    padding: 40rpx;
    background-color: white;
    margin-top: 10rpx;
}

.cell .title {
    font-size: 28rpx;
}

.cell .bottom-wraper {
    margin-top: 20rpx;
    align-self: stretch;
    display: flex;
    justify-content: space-between;
    align-items: center;
}


.cell .bottom-wraper .time {
    font-size: 28rpx;
    color: #999;
}

.cell .bottom-wraper .status {
    color: #7381DC;
    font-size: 30rpx;
    font-weight: bold;
}
